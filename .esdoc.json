{
  "source": "./src",
  "destination": "./docs",
  "excludes": ["src/edit/"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "objectRestSpread": true,
        "accessor": {"access": ["public"], "autoPrivate": false},
        "manual": {
          "asset": "./manual/assets",
          "files": [
            "./manual/doc.md"
          ]
        }
      }
    },
    { "name": "esdoc-exclude-source-plugin" },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": { "objectRestSpread": true }
    }
  ]
}