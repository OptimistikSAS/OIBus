{
  "engine": {
    "engineName": "OIBus",
    "port": 2223,
    "user": "admin",
    "password": "",
    "filter": ["127.0.0.1", "::1", "::ffff:127.0.0.1"],
    "logParameters": {
      "consoleLevel": "debug",
      "fileLevel": "debug",
      "filename": "./logs/journal.log",
      "maxsize": 1000000,
      "maxFiles": 5,
      "tailable": true,
      "sqliteLevel": "debug",
      "sqliteFilename": "./logs/journal.db",
      "sqliteMaxFileSize": 50000000
    },
    "caching": {
      "cacheFolder": "./cache",
      "archiveMode": "archive",
      "archiveFolder": "./cache/archived/"
    },
    "scanModes": [
      {
        "scanMode": "everySecond",
        "cronTime": "* * * * * *"
      },
      {
        "scanMode": "every10Seconds",
        "cronTime": "* * * * * /10"
      },
      {
        "scanMode": "every1Min",
        "cronTime": "* * * * *"
      },
      {
        "scanMode": "every10Minutes",
        "cronTime": "* * * * /10"
      },
      {
        "scanMode": "listen",
        "cronTime": "listen"
      }
    ],
    "proxies" : [],
    "aliveSignal": {
      "enabled": false,
      "host": "",
      "endpoint": "/api/optimistik/oibus/info",
      "authentication": {
        "type": "Basic",
        "username": "",
        "password": ""
      },
      "id": "",
      "frequency": 300,
      "proxy": ""
    },
    "httpRequest": {
      "stack": "fetch",
      "timeout": 30,
      "retryCount": 3
    }
  },
  "south": {
    "dataSources": []
  },
  "north": {
    "applications": []
  },
  "schemaVersion": 3
}
