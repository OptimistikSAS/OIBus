{
  "name": "oibus",
  "version": "1.0.1",
  "main": "index.js",
  "author": "Optimistik SAS",
  "license": "EUPL",
  "scripts": {
    "start": "node src/index.js --config ./tests/oibus.json",
    "start-macos": "./dist/macos/oibus --config ./tests/oibus.json",
    "start-win": ".\\dist\\win\\oibus.exe --config .\\tests\\oibus.json",
    "start-linux": "./dist/linux/oibus --config ./tests/oibus.json",
    "build": "npm ci && npm run build-client && npm run build-win && npm run build-macos && npm run build-linux",
    "build-client": "webpack --mode production",
    "build-win": "npm run build-client && rimraf ./dist/win && pkg ./src/index.js --public --config .pkg.json --output ./dist/win/oibus --target node14-win-x64 && ncp ./deps/win ./dist/win",
    "build-macos": "npm run build-client && rimraf ./dist/macos && pkg ./src/index.js --public --config .pkg.json --output ./dist/macos/oibus --target node14-macos-x64 && ncp ./deps/macos ./dist/macos",
    "build-linux": "npm run build-client && rimraf ./dist/linux && pkg ./src/index.js --public --config .pkg.json --output ./dist/linux/oibus --target node14-linux-x64 && ncp ./deps/linux ./dist/linux",
    "start-client": "webpack --mode development --watch",
    "build-doc": "esdoc",
    "test": "jest --coverage",
    "lint": "eslint src --ext .jsx --ext .js",
    "version": "npm run build && gulp build-archives",
    "postversion": "git push && git push --tags",
    "release": "standard-version -i CHANGELOG.md && git push --follow-tags origin"
  },
  "keywords": [
    "Optimistik"
  ],
  "browserslist": [
    "last 2 chrome versions",
    "last 2 safari versions",
    "last 2 edge versions",
    "last 2 firefox versions",
    "firefox esr"
  ],
  "dependencies": {
    "@babel/runtime": "7.13.7",
    "@fortawesome/fontawesome-free": "5.15.2",
    "@koa/cors": "3.1.0",
    "@koa/multer": "3.0.0",
    "@koa/router": "10.0.0",
    "aws-sdk": "2.849.0",
    "axios": "0.21.1",
    "basic-auth": "2.0.1",
    "bootstrap-less-port": "2.3.1",
    "form-data": "4.0.0",
    "humanize-string": "2.1.0",
    "jsmodbus": "4.0.5",
    "jsondiffpatch": "0.4.1",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-helmet": "6.0.0",
    "koa-json": "2.0.2",
    "koa-logger": "3.2.1",
    "koa-respond": "2.1.0",
    "koa-send": "5.0.1",
    "micromatch": "4.0.2",
    "minimist": "1.2.5",
    "moment-timezone": "0.5.33",
    "mongodb": "3.6.4",
    "mqtt": "4.2.6",
    "mqtt-wildcard": "3.0.9",
    "mssql": "6.3.1",
    "multer": "1.4.2",
    "mysql2": "2.2.5",
    "node-fetch": "2.6.1",
    "node-opcua": "2.33.0",
    "object-path": "0.11.5",
    "oracledb": "^5.1.0",
    "papaparse": "^5.3.0",
    "pg": "8.5.1",
    "prop-types": "15.7.2",
    "proxy-agent": "4.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "4.2.0",
    "react-json-view": "1.21.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "reactstrap": "8.9.0",
    "sprintf-js": "1.1.2",
    "sqlite": "4.0.19",
    "sqlite3": "5.0.2",
    "timexe": "1.0.3",
    "tunnel": "0.0.6",
    "winston": "3.3.3",
    "winston-transport": "4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.1",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.13.0",
    "@babel/plugin-proposal-optional-chaining": "7.13.0",
    "@babel/preset-env": "7.13.0",
    "@babel/preset-react": "7.12.13",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-polyfill": "6.26.0",
    "css-loader": "5.0.2",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-exclude-source-plugin": "1.0.0",
    "esdoc-node": "1.0.5",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "gulp": "4.0.2",
    "gulp-awspublish": "4.1.2",
    "gulp-zip": "5.0.2",
    "html-webpack-plugin": "5.2.0",
    "ignore-loader": "0.1.2",
    "jest": "26.6.3",
    "less": "4.1.1",
    "less-loader": "8.0.0",
    "merge-stream": "2.0.0",
    "ncp": "2.0.0",
    "path-browserify": "1.0.1",
    "pkg": "4.4.9",
    "react-test-renderer": "17.0.1",
    "rimraf": "3.0.2",
    "standard-version": "9.1.1",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.24.0",
    "webpack-cli": "4.5.0"
  },
  "jest": {
    "globalSetup": "./jest-config.js",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
