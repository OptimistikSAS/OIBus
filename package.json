{
  "name": "oibus",
  "private": true,
  "version": "0.4.0",
  "main": "index.js",
  "author": "Optimistik",
  "license": "SEE LICENSE in LICENSE",
  "scripts": {
    "start": "node src/index.js --config ./tests/oibus.json",
    "start-macos": "./dist/macos/oibus --config ./tests/oibus.json",
    "start-win": ".\\dist\\win\\oibus.exe --config .\\tests\\oibus.json",
    "start-linux": "./dist/linux/oibus --config ./tests/oibus.json",
    "build": "npm ci && npm run build-client && npm run build-win && npm run build-macos && npm run build-linux",
    "build-client": "webpack --mode production",
    "build-win": "rimraf ./dist/win && pkg ./src/index.js --config .pkg.json --output ./dist/win/oibus --target node10-win-x64 && ncp ./deps/win ./dist/win",
    "build-macos": "rimraf ./dist/macos && pkg ./src/index.js --config .pkg.json --output ./dist/macos/oibus --target node10-macos-x64 && ncp ./deps/macos ./dist/macos",
    "build-linux": "rimraf ./dist/linux && pkg ./src/index.js --config .pkg.json --output ./dist/linux/oibus --target node10-linux-x64 && ncp ./deps/linux ./dist/linux",
    "start-client": "webpack --mode development --watch",
    "build-doc": "esdoc",
    "test": "jest",
    "lint": "eslint src --ext .jsx --ext .js",
    "version": "npm run build && gulp build-archives",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "Optimistik"
  ],
  "dependencies": {
