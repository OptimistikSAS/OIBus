  "scripts": {
    "start": "node src/index.js --config ./tests/oibus.json",
    "start-macos": "./dist/oibus-macos --config ./tests/oibus.json",
    "start-win": ".\\dist\\oibus-win.exe --config .\\tests\\oibus.json",
    "start-linux": "./dist/oibus-linux --config ./tests/oibus.json",
    "build": "npm run build-client && npm run build-win && npm run build-macos && npm run build-linux",
    "build-client": "webpack --mode production",
    "build-win": "pkg ./src/index.js --config .pkg.json --output ./dist/oibus-win --target node10-win-x64",
    "build-macos": "pkg ./src/index.js --config .pkg.json --output ./dist/oibus-macos --target node10-macos-x64",
    "build-linux": "pkg ./src/index.js --config .pkg.json --output ./dist/oibus-linux --target node10-linux-x64",
    "start-client": "webpack --mode development --watch",
    
    "build-doc": "esdoc",
    "test": "jest",
    "lint": "eslint src --ext .jsx --ext .js"
  },
  "keywords": [
    "factoryThings"
  ],
