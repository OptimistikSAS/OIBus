    "react-icons": "3.9.0",
    "react-json-view": "1.19.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "reactstrap": "8.4.1",
    "request-promise-native": "1.0.8",
    "sqlite": "3.0.3",
