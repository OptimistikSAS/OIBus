{
  "name": "oibus",
  "version": "1.2.4",
  "main": "index.js",
  "author": "Optimistik SAS",
  "license": "EUPL",
  "engines": {
    "node": ">=v16.13.0",
    "npm": ">=8.1.0"
  },
  "scripts": {
    "start": "node src/index.js --config ./tests/oibus.json",
    "start-macos": "./dist/macos/oibus --config ./tests/oibus.json",
    "start-win": ".\\dist\\win\\oibus.exe --config .\\tests\\oibus.json",
    "start-linux": "./dist/linux/oibus --config ./tests/oibus.json",
    "build": "npm ci && npm run build-win && npm run build-macos && npm run build-linux",
    "build-client": "webpack --mode production",
    "build-lib-win": "node-pre-gyp --directory ./node_modules/sqlite3 install --target_platform=win32",
    "build-lib-macos": "node-pre-gyp --directory ./node_modules/sqlite3 install --target_platform=darwin",
    "build-lib-linux": "node-pre-gyp --directory ./node_modules/sqlite3 install --target_platform=linux",
    "build-win": "npm run build-client && npm run build-lib-win && rimraf ./dist/win && pkg ./src/index.js --public --compress GZip --config .pkg.json --output ./dist/win/oibus --target node14-win-x64 && ncp ./deps/win ./dist/win",
    "build-macos": "npm run build-client && npm run build-lib-macos && rimraf ./dist/macos && pkg ./src/index.js --public --compress GZip --config .pkg.json --output ./dist/macos/oibus --target node14-macos-x64",
    "build-linux": "npm run build-client && npm run build-lib-linux && rimraf ./dist/linux && pkg ./src/index.js --public --compress GZip --config .pkg.json --output ./dist/linux/oibus --target node14-linux-x64",
    "build-win-setup": "rimraf ./dist/win/win-setup-release && C:\\\"Program Files (x86)\"\\\"Inno Setup 6\"\\iscc.exe \"./deps/win/oibus_setup.iss\" \"/DMyAppVersion=\"%npm_package_version%",
    "start-client": "webpack --mode development --watch",
    "test": "jest --coverage",
    "lint": "eslint src --ext .jsx --ext .js",
    "version": "npm run build && gulp build-archives",
    "postversion": "git push && git push --tags",
    "bump": "standard-version -i CHANGELOG.md",
    "postbump": "git push --follow-tags origin",
    "setup-dev-env": "cd local-development && docker-compose --env-file ./.env.dev up",
    "seed-destinations": "node ./local-developent/database-seeder/seed-db.js"
  },
  "keywords": [
    "Optimistik"
  ],
  "browserslist": [
    "last 2 chrome versions",
    "last 2 safari versions",
    "last 2 edge versions",
    "last 2 firefox versions",
    "firefox esr"
  ],
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@koa/multer": "3.0.0",
    "@koa/router": "10.1.1",
    "ads-client": "1.11.4",
    "aws-sdk": "2.1021.0",
    "axios": "0.24.0",
    "basic-auth": "2.0.1",
    "bootstrap": "4.6.1",
    "form-data": "4.0.0",
    "humanize-string": "2.1.0",
    "jsmodbus": "4.0.6",
    "jsondiffpatch": "0.4.1",
    "koa": "2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-helmet": "6.1.0",
    "koa-json": "2.0.2",
    "koa-logger": "3.2.1",
    "koa-respond": "2.1.0",
    "koa-send": "5.0.1",
    "micromatch": "4.0.4",
    "minimist": "1.2.5",
    "moment-timezone": "0.5.33",
    "mongodb": "4.1.4",
    "mqtt": "4.2.8",
    "mqtt-wildcard": "3.0.9",
    "mssql": "7.2.1",
    "mysql2": "2.3.2",
    "nanoid": "3.1.30",
    "node-fetch": "2.6.6",
    "node-opcua": "2.50.0",
    "object-path": "0.11.8",
    "oracledb": "5.3.0",
    "papaparse": "5.3.1",
    "pg": "8.7.1",
    "prop-types": "15.7.2",
    "proxy-agent": "5.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flow-renderer": "9.6.11",
    "react-icons": "4.3.1",
    "react-json-view": "1.21.3",
    "react-router": "6.0.1",
    "react-router-dom": "6.0.1",
    "reactstrap": "8.10.1",
    "sprintf-js": "1.1.2",
    "sqlite": "4.0.23",
    "sqlite3": "5.0.2",
    "timexe": "1.0.5",
    "tunnel": "0.0.6",
    "winston": "3.3.3",
    "winston-loki": "6.0.2",
    "winston-transport": "4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.16.0",
    "@babel/plugin-proposal-optional-chaining": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "babel-polyfill": "6.26.0",
    "css-loader": "6.5.1",
    "eslint": "8.1.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.3.1",
    "ncp": "2.0.0",
    "path-browserify": "1.0.1",
    "pkg": "5.4.1",
    "rimraf": "3.0.2",
    "standard-version": "9.3.2",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "webpack": "5.61.0",
    "webpack-cli": "4.9.1"
  },
  "jest": {
    "globalSetup": "./jest-config.js",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
