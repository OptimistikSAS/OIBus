{
  "name": "oibus",
  "private": true,
  "version": "0.6.0-dev",
  "main": "index.js",
  "author": "Optimistik",
  "license": "SEE LICENSE in LICENSE",
  "scripts": {
    "start": "node src/index.js --config ./tests/oibus.json",
    "start-macos": "./dist/macos/oibus --config ./tests/oibus.json",
    "start-win": ".\\dist\\win\\oibus.exe --config .\\tests\\oibus.json",
    "start-linux": "./dist/linux/oibus --config ./tests/oibus.json",
    "build": "npm ci && npm run build-client && npm run build-win && npm run build-macos && npm run build-linux",
    "build-client": "webpack --mode production",
    "build-win": "rimraf ./dist/win && pkg ./src/index.js --config .pkg.json --output ./dist/win/oibus --target node12-win-x64 && ncp ./deps/win ./dist/win",
    "build-macos": "rimraf ./dist/macos && pkg ./src/index.js --config .pkg.json --output ./dist/macos/oibus --target node12-macos-x64 && ncp ./deps/macos ./dist/macos",
    "build-linux": "rimraf ./dist/linux && pkg ./src/index.js --config .pkg.json --output ./dist/linux/oibus --target node12-linux-x64 && ncp ./deps/linux ./dist/linux",
    "start-client": "webpack --mode development --watch",
    "build-doc": "esdoc",
    "test": "jest --coverage",
    "lint": "eslint src --ext .jsx --ext .js",
    "version": "npm run build && gulp build-archives",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "Optimistik"
  ],
  "browserslist": [
    "defaults",
    "edge >= 14"
  ],
  "dependencies": {
    "@babel/runtime": "7.10.2",
    "@fortawesome/fontawesome-free": "5.13.0",
    "@koa/cors": "3.1.0",
    "@koa/router": "9.0.1",
    "aws-sdk": "2.692.0",
    "axios": "0.19.2",
    "basic-auth": "2.0.1",
    "bootstrap-less-port": "2.3.1",
    "check-disk-space": "2.1.0",
    "fast-csv": "4.3.0",
    "form-data": "3.0.0",
    "humanize-string": "2.1.0",
    "jsmodbus": "4.0.4",
    "jsondiffpatch": "0.4.1",
    "koa": "2.12.0",
    "koa-bodyparser": "4.3.0",
    "koa-helmet": "5.2.0",
    "koa-json": "2.0.2",
    "koa-logger": "3.2.1",
    "koa-respond": "2.1.0",
    "koa-send": "5.0.0",
    "micromatch": "4.0.2",
    "minimist": "1.2.5",
    "moment-timezone": "0.5.31",
    "mongodb": "3.5.8",
    "mqtt": "4.1.0",
    "mqtt-wildcard": "3.0.9",
    "mssql": "6.2.0",
    "mysql2": "2.1.0",
    "node-fetch": "2.6.0",
    "node-opcua": "2.6.6",
    "object-path": "0.11.4",
    "oracledb": "4.2.0",
    "pg": "8.2.1",
    "prop-types": "15.7.2",
    "proxy-agent": "3.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "3.10.0",
    "react-json-view": "1.19.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "reactstrap": "8.4.1",
    "sprintf-js": "1.1.2",
    "sqlite": "4.0.10",
    "sqlite3": "4.2.0",
    "timexe": "1.0.0",
    "tunnel": "0.0.6",
    "winston": "3.2.1",
    "winston-transport": "4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-polyfill": "6.26.0",
    "css-loader": "3.5.3",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-exclude-source-plugin": "1.0.0",
    "esdoc-node": "1.0.5",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.21.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "gulp": "4.0.2",
    "gulp-awspublish": "4.1.2",
    "gulp-zip": "5.0.1",
    "html-webpack-plugin": "4.3.0",
    "ignore-loader": "0.1.2",
    "jest": "26.0.1",
    "less": "3.11.3",
    "less-loader": "6.1.0",
    "merge-stream": "2.0.0",
    "ncp": "2.0.0",
    "pkg": "4.4.8",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "style-loader": "1.2.1",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "jest": {
    "globalSetup": "./jest-config.js",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
